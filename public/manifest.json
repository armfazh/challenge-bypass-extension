{
    "name": "Privacy Pass: The Client",
    "manifest_version": 2,
    "description": "Client support for Privacy Pass anonymous authorization protocol.",
    "version": "4.0.0",
    "icons": {
        "32": "icons/32/gold.png",
        "48": "icons/48/gold.png",
        "64": "icons/64/gold.png",
        "128": "icons/128/gold.png"
    },
    "background": {
        "scripts": [
            "svcwork.js"
        ]
    },
    "permissions": [
        "<all_urls>",
        "activeTab",
        "storage",
        "webRequest",
        "webRequestBlocking"
    ],
    "browser_action": {
        "default_icon": "icons/32/grey.png",
        "default_title": "Privacy Pass: The Client",
        "default_popup": "popup.html"
    }
}
